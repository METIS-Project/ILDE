<metadata>
  <schema>IMS Common Cartridge</schema>
  <schemaversion>1.0.0</schemaversion>
  <lom xmlns="http://ltsc.ieee.org/xsd/imscc/LOM">
    <general>
      <title><string>%(title)s</string></title>
      <description><string>%(description)s</string></description>
    </general>
    <lifeCycle>
      <contribute>
        <date>
          <dateTime>%(date)s</dateTime>
	</date>
	<entity>%(creator)s</entity>
      </contribute>
      <contribute>
        <role>
          <value>author</value>
	</role>
	<entity>%(creator)s</entity>
      </contribute>
      <contribute>
        <role>
          <value>publisher</value>
	</role>
	<entity>%(publisher)s</entity>
      </contribute>
    </lifeCycle>
    <technical>
      <format>text/html</format>
    </technical>
    <rights>
      <copyrightAndOtherRestrictions>
        <value>yes</value>
      </copyrightAndOtherRestrictions>
      <description><string>%(rights)s</string></description>
    </rights>
  </lom>
</metadata>
